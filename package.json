{
  "name": "sendable-error",
  "version": "0.3.1",
  "main": "cjs/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "bun run test:types && bun run check && bun run test:unit",
    "test:unit": "bun test",
    "test:types": "tsc --noEmit -p .",
    "prepublishOnly": "bun run test && bun run build",
    "example:start": "cd example && npm run start",
    "example:dev": "cd example && npm run dev",
    "check:fix": "bun x @biomejs/biome check --apply-unsafe src",
    "check": "bun x @biomejs/biome check src",
    "cleanup": "rimraf esm && rimraf cjs",
    "build": "bun run cleanup && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2015 --outDir esm",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "build:watch": "bun run cleanup && npm-run-all -p \"build:esm -- -w\" \"build:cjs -- -w\""
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "bun": "./src/index.ts"
    },
    "./esm": "./esm/index.js",
    "./cjs": "./cjs/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "@biomejs/biome": "^1.4.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "esm/",
    "cjs/",
    "src/"
  ],
  "author": "James Waterhouse <09jwater@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/Censkh/sendable-error",
  "keywords": [
    "typescript",
    "error",
    "api"
  ]
}
